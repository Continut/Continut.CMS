{
  "ThemeContinutOrg": {
    "version": "0.0.1",
    "name": "Continut.org main theme",
    "folder": "ThemeContinutOrg",
    "type": "Local",
    "description": "The official theme from continut.org",
    "author": {
      "web": "www.pixelplant.ch",
      "name": "Radu Mogo≈ü",
      "email": "radu.mogos@pixelplant.ch"
    },
    "dependencies": {
      "sys_backend": {
        "version": "0.0.1"
      }
    },
    "ui": {
      "layout": {
        "default": {
          "label": "layout.default",
          "backendFile": "/Extensions/Local/ThemeContinutOrg/Resources/Private/Backend/Layouts/Default.layout.php",
          "frontendFile": "/Extensions/Local/ThemeContinutOrg/Resources/Private/Frontend/Layouts/Default.layout.php"
        }
      }
    },
    "elements": {
      "content": {
        "textWithImage": {
          "label": "backend.themeContinutOrg.type.content.textWithImage.title",
          "description": "backend.themeContinutOrg.type.content.textWithImage.description",
          "icon": "Icons/text_with_image.png",
          "template": "TextWithImage"
        },
        "iconText": {
          "label": "backend.themeContinutOrg.type.content.iconText.title",
          "description": "backend.themeContinutOrg.type.content.iconText.description",
          "icon": "Icons/icon_text.png",
          "template": "IconText"
        },
        "video": {
          "label": "backend.themeContinutOrg.type.content.video.title",
          "description": "backend.themeContinutOrg.type.content.video.description",
          "icon": "Icons/video.png",
          "template": "Video"
        },
        "jumbotron": {
          "label": "backend.themeContinutOrg.type.content.jumbotron.title",
          "description": "backend.themeContinutOrg.type.content.jumbotron.description",
          "icon": "Icons/jumbotron.png",
          "template": "jumbotron"
        }
      },
      "container": {
        "2columns": {
          "label": "backend.themeContinutOrg.type.container.2columns.title",
          "description": "backend.themeContinutOrg.type.container.2columns.description",
          "icon": "Icons/container_2columns.png",
          "template": "2columns"
        },
        "3columns": {
          "label": "backend.themeContinutOrg.type.container.3columns.title",
          "description": "backend.themeContinutOrg.type.container.3columns.description",
          "icon": "Icons/container_3columns.png",
          "template": "3columns"
        }
      },
      "plugin": {
        "notificationEmail": {
          "label": "backend.themeContinutOrg.type.plugin.notificationEmail.title",
          "description": "backend.themeContinutOrg.type.plugin.notificationEmail.description",
          "icon": "Icons/notificationEmail.png",
          "template": "notification",
          "frontend": {
            "controller": "Email",
            "action": "notification"
          },
          "backend": {
            "controller": "Preview",
            "action": "notificationPreview"
          }
        }
      }
    }
  }
}