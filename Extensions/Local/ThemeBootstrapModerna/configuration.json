        },
        "4columns": {
          "label": "backend.themeBootstrapModerna.type.container.4columns.title",
          "description": "backend.themeBootstrapModerna.type.container.4columns.description",
          "icon": "Icons/container_4columns.png",
          "template": "4columns"
        }
      }
