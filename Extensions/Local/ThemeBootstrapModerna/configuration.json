          "label": "backend.themeBootstrapModerna.type.container.1column.title",
          "description": "backend.themeBootstrapModerna.type.container.1column.description",
          "icon": "Icons/container_1column.png",
          "template": "1column"
        },
        "2columns": {
          "label": "backend.themeBootstrapModerna.type.container.2columns.title",
          "description": "backend.themeBootstrapModerna.type.container.2columns.description",
          "icon": "Icons/container_2columns.png",
