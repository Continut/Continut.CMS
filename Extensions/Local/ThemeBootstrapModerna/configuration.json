{
  "ThemeBootstrapModerna": {
    "version": "0.0.1",
    "name": "Moderna - Bootstrap Theme",
    "folder": "ThemeBootstrapModerna",
    "type": "Local",
    "description": "A complete bootstrap responsive theme that you can use for your website",
    "author": {
      "web": "www.pixelplant.ch",
      "name": "Radu Mogo≈ü",
      "email": "radu.mogos@pixelplant.ch"
    },
    "dependencies": {
      "sys_backend": {
        "version": "0.0.1"
      }
    },
    "ui": {
      "layout": {
        "default": {
          "label": "layout.default",
          "backendFile": "/Extensions/Local/ThemeBootstrapModerna/Resources/Private/Backend/Layouts/Default.layout.php",
          "frontendFile": "/Extensions/Local/ThemeBootstrapModerna/Resources/Private/Frontend/Layouts/Default.layout.php"
        },
        "homepage": {
          "label": "layout.homepage",
          "backendFile": "/Extensions/Local/ThemeBootstrapModerna/Resources/Private/Backend/Layouts/Homepage.layout.php",
          "frontendFile": "/Extensions/Local/ThemeBootstrapModerna/Resources/Private/Frontend/Layouts/Homepage.layout.php"
        }
      }
    },
    "elements": {
      "contents": {
        "textWithImage": {
          "label": "backend.themeBootstrapModerna.type.content.textWithImage.title",
          "description": "backend.themeBootstrapModerna.type.content.textWithImage.description",
          "icon": "Icons/text_with_image.png",
          "template": "TextWithImage"
        },
        "box": {
          "label": "backend.themeBootstrapModerna.type.content.box.title",
          "description": "backend.themeBootstrapModerna.type.content.box.description",
          "icon": "Icons/box.png",
          "template": "Box"
        },
        "callAction": {
          "label": "backend.themeBootstrapModerna.type.content.callAction.title",
          "description": "backend.themeBootstrapModerna.type.content.callAction.description",
          "icon": "Icons/call_action.png",
          "template": "CallAction"
        },
        "solidLine": {
          "label": "backend.themeBootstrapModerna.type.content.solidLine.title",
          "description": "backend.themeBootstrapModerna.type.content.solidLine.description",
          "icon": "Icons/solid_line.png",
