{
  "ThemeHdv": {
    "version": "0.0.1",
    "name": "Valais Hospital Theme",
    "folder": "ThemeHdv",
    "type": "Local",
    "description": "Theme for HDV - used as a test",
    "author": {
      "web": "www.pixelplant.ch",
      "name": "Radu Mogo≈ü",
      "email": "radu.mogos@pixelplant.ch"
    },
    "dependencies": {
      "sys_backend": {
        "version": "0.0.1"
      }
    },
    "ui": {
      "layout": {
        "default": {
          "label": "layout.default",
          "backendFile": "/Extensions/Local/ThemeHdv/Resources/Private/Backend/Layouts/Default.layout.php",
          "frontendFile": "/Extensions/Local/ThemeHdv/Resources/Private/Frontend/Layouts/Default.layout.php"
        }
      }
    },
    "elements": {
      "container": {
        "wrapper": {
          "label": "backend.themeHdv.type.container.wrapper.wizardTitle",
          "description": "backend.themeHdv.type.container.wrapper.wizardDescription",
          "icon": "Icons/container_wrapper.png",
          "template": "Wrapper"
        }
      },
      "content": {
        "swiperHeader": {
          "label": "backend.themeHdv.type.content.swiperHeader.wizardTitle",
          "description": "backend.themeHdv.type.content.swiperHeader.wizardDescription",
          "icon": "Icons/swiperHeader.png",
          "template": "SwiperHeader"
        },
        "media": {
          "label": "backend.themeHdv.type.content.media.wizardTitle",
          "description": "backend.themeHdv.type.content.media.wizardDescription",
          "icon": "Icons/media.png",
          "template": "Media"
        }
      }
    }
  }
}